{
    "version": 2,
    "builds": [
      {
        "src": "online_assignment_management/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "online_assignment_management/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles_build"
    }